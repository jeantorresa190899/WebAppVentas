@{
}

<h4>Registrar un nuevo distrito</h4>
<form asp-controller="Distrito" asp-action="CreateDistrito" method="post">
    <div class="form-group">
        <label>Codigo</label>
        <input asp-for="CodDis" class="form-control" placeholder="Codigo" autofocus/>
        <span asp-validation-for="CodDis" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label>Nombre Distrito</label>
        <input asp-for="NomDis" class="form-control" placeholder="..." />
        <span asp-validation-for="NomDis" class="text-danger"></span>
    </div>
    <div class="form-group">
    <button type="submit" class="btn btn-primary" value="btn">Grabar</button>
    </div>
</form>
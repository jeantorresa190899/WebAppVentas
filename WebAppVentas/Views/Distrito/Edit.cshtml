@{
}


<h4>EDITAR un Distrito</h4>
<form asp-controller="Distrito" asp-action="EditDistrito" method="post">
    <div class="form-group">
        <label>Codigo</label>
        <input asp-for="CodDis" value="@ViewData["cod"]" class="form-control" placeholder="Codigo" autofocus />
        <span asp-validation-for="CodDis" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label>Nombre </label>
        <input asp-for="NomDis" value="@ViewData["nom"]" class="form-control" placeholder="..." />
        <span asp-validation-for="NomDis" class="text-danger"></span>
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-primary" value="btn">Modificar y grabar</button>
    </div>
</form>